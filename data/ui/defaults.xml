<?xml version="1.0" encoding="UTF-8"?>

<Defaults>
    <VBox>
        <EditorMenu
            skinName="'EditorPanel'"
            paddingBottom = "10"
        />
        <EditorWidgetProperties
            align           = "'top,left'"
            paddingBottom      = "20"
            childPadding    = "0"
            autoSize        = "true"
            skinName        = "'matBut'"
        />
    </VBox>
    <Text>
        <Default
            format-font      = "$Assets.getFont('fonts/LibreBaskerville-Regular.ttf').fontName"
            format-size      = "8"
            label-embedFonts = "true"
            label-selectable = "false"
        />
        <budgetD
            format-font      = "$Assets.getFont('fonts/LibreBaskerville-Regular.ttf').fontName"
            format-size      = "12"
            label-embedFonts = "true"
            label-selectable = "false"
        />
        <EditorCmpLabel
            format-font      = "$Assets.getFont('fonts/CONSOLA.TTF').fontName"
            format-size      = "12"
            label-embedFonts = "true"
            label-selectable = "false"
            skinName         = "'EditorRow'"
        />
        <EditorHeading
            format-font      = "$Assets.getFont('fonts/CONSOLA.TTF').fontName"
            format-size      = "18"
            label-embedFonts = "true"
            label-selectable = "false"
            skinName         = "'EditorHeadingSkin'"
        />
    </Text>
    <InputText>
        <Default
            label-filters    = "[new openfl.filters.GlowFilter(0x333333, 1, 2, 2)]"
            format-font      = "$Assets.getFont('fonts/LibreBaskerville-Regular.ttf').fontName"
            format-size      = "8"
            label-autoSize   = "openfl.text.TextFieldAutoSize.NONE"
            label-embedFonts = "true"
            w           = "200"
            label-width = "200"
            skin:Paint-border      = "2"
            skin:Paint-borderColor = "0xcdc4d9"
            skin:Paint-color       = "0xFFFFFF"
        />
    </InputText>
    <MyInputText>
        <EditorCmpField
            format-font      = "$Assets.getFont('fonts/CONSOLA.TTF').fontName"
            format-size      = "12"
            label-embedFonts = "true"
            label-selectable = "true"
            skinName         = "'EditorRow'"
            w                = "120"
            autoHeight       = "true"
        />
    </MyInputText>

    <Button>
        <Default
            padding = "5"
            h       = "30"
            label-selectable   = "false"
            label-mouseEnabled = "false"
            label-embedFonts   = "true"
            label-autoSize     = "openfl.text.TextFieldAutoSize.LEFT"
            format-font        = "$Assets.getFont('fonts/LibreBaskerville-Regular.ttf').fontName"
            format-size        = "14"
            format-color       = "0xcdc4d9  "
            format-align       = "openfl.text.TextFormatAlign.CENTER"
        />
        <EditorButton
            label-selectable   = "false"
            label-mouseEnabled = "false"
            label-embedFonts   = "true"
            label-autoSize     = "openfl.text.TextFieldAutoSize.LEFT"
            format-font        = "$Assets.getFont('fonts/CONSOLA.TTF').fontName"
            format-size        = "14"
            format-color       = "0xcdc4d9  "
            format-align       = "openfl.text.TextFormatAlign.LEFT"
        />
        <EditorMenu
            label-selectable   = "false"
            label-mouseEnabled = "false"
            label-embedFonts   = "true"
            label-autoSize     = "openfl.text.TextFieldAutoSize.LEFT"
            format-font        = "$Assets.getFont('fonts/CONSOLA.TTF').fontName"
            format-size        = "14"
            format-color       = "0xcdc4d9"
            format-align       = "openfl.text.TextFormatAlign.LEFT"
        />
    </Button>
    
    <Scroll>
        <EditorCmpPanel
            w           = "200"
            h           = "600"
            skinName    = "'EditorPanel'"
            vBar-slider-skin:Paint-color="0x002200"
            hBar-visible = "false"
        />
    </Scroll>
    <TabPage>
        <EditorMenu
            title-skinName    = "'EditorMenuTab'"
            title-w           = "100"
            title-h           = "50"
            title-label-selectable   = "false"
            title-label-mouseEnabled = "false"
            title-label-embedFonts   = "true"
            title-label-autoSize     = "openfl.text.TextFieldAutoSize.LEFT"
            title-format-font        = "$Assets.getFont('fonts/CONSOLA.TTF').fontName"
            title-format-size        = "14"
            title-format-color       = "0xcdc4d9  "
            title-format-align       = "openfl.text.TextFormatAlign.LEFT"
        />
    </TabPage>
</Defaults>